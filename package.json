    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "smui-theme": "^6.0.0-beta.13",
    "svelte": "^3.44.0",
    "svelte-body": "^1.3.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
