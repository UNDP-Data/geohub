    "prettifile": "prettier --ignore-path .prettierignore --write",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/styles/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/styles/theme/dark",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark && husky install",
    "test": "vitest --run --dir src --passWithNoTests",
    "test:watch": "vitest --watch --dir src --passWithNoTests",
    "ci:compile": "node --max_old_space_size=4096 ./node_modules/vite/bin/vite.js build",
