{
  "name": "geohub",
  "version": "0.0.2",
  "description": "GeoHub is a geospatial visualization data portal built by UNDP using sveltekit",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check  \"src/**/*.{ts,tsx,js,html,svelte}\" --ignore-path ./.prettierignore && eslint ./src --fix",
    "format": "prettier --plugin-search-dir . --write \"src/**/*.{ts,tsx,js,html,svelte}\" --ignore-path ./.prettierignore",
    "test": "vitest --run --dir src --passWithNoTests",
    "test:watch": "vitest --watch --dir src --passWithNoTests",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [
    "geohub",
    "undp",
    "maplibre-gl-js",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UNDP-Data/geohub.git"
  },
  "author": "United Nations Development Programme",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/UNDP-Data/geohub/issues"
  },
  "homepage": "https://github.com/UNDP-Data/geohub#readme",
