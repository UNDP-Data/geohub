{
	"name": "@undp-data/svelte-undp-design",
	"version": "0.0.2",
	"description": "This package is to manage svelte components for UNDP design system in GeoHub",
	"main": "package/index.js",
	"files": [
		"package"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "pnpm build",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build -o ../../sites/geohub/static/storybook"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/UNDP-Data/geohub.git"
	},
	"author": "United Nations Development Programme",
	"license": "BSD-3-Clause",
	"bugs": {
		"url": "https://github.com/UNDP-Data/geohub/issues"
	},
	"homepage": "https://github.com/UNDP-Data/geohub#readme",
	"devDependencies": {
		"@storybook/addon-essentials": "7.0.0-beta.12",
		"@storybook/addon-interactions": "7.0.0-beta.12",
		"@storybook/addon-links": "7.0.0-beta.12",
		"@storybook/blocks": "7.0.0-beta.12",
		"@storybook/svelte": "7.0.0-beta.12",
		"@storybook/sveltekit": "7.0.0-beta.12",
		"@storybook/testing-library": "^0.0.13",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.11",
		"@sveltejs/package": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"bulma": "^0.9.4",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-storybook": "^0.6.9",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.2",
		"prettier-plugin-svelte": "^2.9.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.57.1",
		"storybook": "7.0.0-beta.12",
		"svelte-check": "^2.10.3",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"type": "module",
	"dependencies": {
		"svelte": "^3.55.0",
		"svelte-carousel": "^1.0.20"
	},
	"peerDependencies": {
		"svelte": "^3.55.0"
	}
}
