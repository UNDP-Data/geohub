{
  "name": "@undp-data/undp-bulma",
  "version": "1.0.3",
  "description": "This package is to manage customised Bulma css for UNDP GeoHub",
  "type": "module",
  "main": "bulma.scss",
  "files": [
    "dist",
    "!dist/**/*.js",
    "!dist/**/*.cjs",
    "package.json",
    "README.md"
  ],
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && copyfiles -u 2 'assets/sdgs/*' dist/assets/sdgs",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UNDP-Data/geohub.git"
  },
  "author": "United Nations Development Programme",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/UNDP-Data/geohub/issues"
  },
  "homepage": "https://github.com/UNDP-Data/geohub#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  },
  "dependencies": {
    "bulma": "^1.0.0",
    "sass": "^1.75.0"
  }
}
